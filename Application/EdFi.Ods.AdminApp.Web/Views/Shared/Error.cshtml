@{
    ViewBag.Title = "Error";
}

<div class="row">
    <div class="col-md-12">
        <h1>Error!</h1>
        <h4 id="errorDescription">There was an error processing your request. Please check the system log for diagnostic information.</h4>
        <br />
    </div>
</div>
<div class="row">
    <div class="padding-top-5 text-center">
        <a href="javascript:history.go(-1);" class="btn btn-primary cta">Back</a>
        <a href="#" id="error-report-button" class="btn btn-primary cta hidden">Report Error</a>
    </div>
</div>
