@using EdFi.Ods.AdminApp.Web.Helpers
@using EdFi.Ods.AdminApp.Web.Models.ViewModels.Reports
@model ReportsIndexModel

@{
    ViewBag.Title = "Ods Instance Reports | Total Enrollment";
    Layout = "~/Views/Shared/_Settings_Production.cshtml";
}

@Html.NavPills(Url, Model.OdsInstanceSettingsTabEnumerations)

<div class="padding-bottom-10" align="center">
    <h2>@Model.TotalEnrollmentReport.LocalEducationAgencyName</h2>
</div>

<h3>Total Enrollment</h3>
<table class="table table-condensed table-left-border">
    <tbody>
        <tr>
            <th>Total Enrollment</th>
            <td>@Model.TotalEnrollmentReport.EnrollmentCount</td>
        </tr>
    </tbody>
</table>


<a href="@Url.Action(" SelectDistrict", "OdsInstanceSettings" , new {id=Model.TotalEnrollmentReport.LocalEducationAgencyId})">@Html.Button("Back to Reports")</a>
